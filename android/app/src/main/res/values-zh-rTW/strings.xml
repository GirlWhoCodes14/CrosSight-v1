<resources>
    <string name="app_name">CrosSight</string>
    <string name="settings_button_description">設定</string>

    <string name="go">行</string>
    <string name="stop">停</string>
    <string name="crosswalk">人行道</string>
    <string name="visual_cue">螢幕視覺</string>
    <string name="visual_description">顯示螢幕上的視覺元素，例如顏色和文字，以指示何時以及如何穿越馬路。</string>

